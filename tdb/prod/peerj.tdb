{

  publisher <
    name = PeerJ
  >

  rights = openaccess

  {
  
    title < 
      name = PeerJ ;
      issn = 2167-8359
    >    
  
    plugin = org.lockss.plugin.peerj.PeerJPlugin
    param[base_url] = https://peerj.com/
    implicit < status ; year ; name ; param[volume_name] >
    
    au < ready ; 2013 ; PeerJ 2013 ; 2013 >
    au < exists ; 2014 ; PeerJ 2014 ; 2014 >
    au < expected ; 2015 ; PeerJ 2015 ; 2015 >
    
  }
  
  {
  
    title < 
      name = PeerJ PrePrint Archives ;
      issn = 2167-9843
    >

    plugin = org.lockss.plugin.peerj.PeerJPreprintsPlugin
    param[base_url] = https://peerj.com/
    implicit < status ; year ; name ; param[volume_name] >
    
    au < doNotProcess ; 2013 ; PeerJ PrePrint Archives: 2013 ; 2013 > 
    au < doNotProcess ; 2014 ; PeerJ PrePrint Archives: 2014 ; 2014 > 

  }

}
