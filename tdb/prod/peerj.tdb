{

  publisher <
    name = PeerJ
  >

  rights = openaccess
  param[base_url] = https://peerj.com/
  implicit < status ; year ; name ; param[volume_name] >

  {
  
    title < 
      name = PeerJ ;
      eissn = 2167-8359
    >    
    {
    plugin = org.lockss.plugin.peerj.PeerJPlugin
    
    au < doNotProcess ; 2013 ; PeerJ Volume 2013 ; 2013 >
    }
    {
    plugin = org.lockss.plugin.peerj.PeerJ2016Plugin
    param[journal_id] = peerj 

    au < exists ; 2013 ; PeerJ Volume 2013 ; 2013 >
    au < exists ; 2014 ; PeerJ Volume 2014 ; 2014 >
    au < expected ; 2015 ; PeerJ Volume 2015 ; 2015 >
    au < expected ; 2016 ; PeerJ Volume 2016 ; 2016 >
    }
  }
  
  {

    title < 
      name = PeerJ Computer Science ;
      eissn = 2376-5992
    >

    {

    plugin = org.lockss.plugin.peerj.PeerJ2016Plugin
    param[journal_id] = cs 
    
    au < exists ; 2015 ; PeerJ Computer Science Volume 2015 ; 2015 >
    au < exists ; 2016 ; PeerJ Computer Science Volume 2016 ; 2016 >
    }
  }
}
