{
  publisher <
    name = Martha Heasley Cox Center for Steinbeck Studies
  >

  implicit < status ; year ; name ; param[volume] >

  plugin = org.lockss.plugin.projmuse.ProjectMusePlugin

  {
    title <
      name = Steinbeck Studies ;
      issn = 1551-6903 ;
      eissn = 0898-7734 ;
      issnl = 1551-6903
    >

    param[journal_dir] = steinbeck_studies
    attr[journal_id] = stn

    {

      param[base_url] = http://muse.jhu.edu/

      au < released ; 2004 ; Steinbeck Studies Volume 15 ; 15 >
      au < released ; 2005 ; Steinbeck Studies Volume 16 ; 16 >

    }

 
  }

  {
    title <
      name = Steinbeck Review ;
      issn = 1546-007X ;
      eissn = 1754-6087 ;
      issnl = 1546-007X
    >

    param[journal_dir] = steinbeck_review
    attr[journal_id] = str
    comment[0] = no issue from volume 5 to 9

    {

      param[base_url] = http://muse.jhu.edu/

      au < released ; 2006 ; Steinbeck Review Volume 3 ; 3 >
      au < released ; 2007 ; Steinbeck Review Volume 4 ; 4 >

    }

    {

      param[base_url] = https://muse.jhu.edu/

      au < released ; 2013 ; Steinbeck Review Volume 10 ; 10 >
      au < released ; 2014 ; Steinbeck Review Volume 11 ; 11 >
      au < released ; 2015 ; Steinbeck Review Volume 12 ; 12 >

    }


  }

}
