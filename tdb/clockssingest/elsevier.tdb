{

  publisher <
    name = Elsevier ;
    info[contract] = 2008 ;
    info[tester] = S
  >
  
  {
  
    title <
      name = Elsevier Source Content
    >
    
    {
    
      plugin = org.lockss.plugin.elsevier.ClockssElsevierSourcePlugin
      param[base_url] = http://clockss-ingest.lockss.org/sourcefiles/elsevier-released/
      hidden[proxy] = DIRECT
      implicit < status ; status2 ; year ; name ; param[year] >
      
      # 2008 not on oclc or nii - down/not crawling
      au < readySource ; crawling ; 2008 ; Elsevier Source Content 2008 ; 2008 >
      au < readySource ; frozen ; 2009 ; Elsevier Source Content 2009 ; 2009 >
      au < readySource ; frozen ; 2010 ; Elsevier Source Content 2010 ; 2010 >
      au < readySource ; frozen ; 2011 ; Elsevier Source Content 2011 ; 2011 >
      au < readySource ; crawling ; 2012 ; Elsevier Source Content 2012 ; 2012 >
      au < readySource ; frozen ; 2013 ; Elsevier Source Content 2013 ; 2013 >
      au < readySource ; crawling ; 2014 ; Elsevier Source Content (legacy) 2014 ; 2014 >

    }

    {

      plugin = org.lockss.plugin.elsevier.ClockssElsevierDTD5SourcePlugin
      param[base_url] = http://clockss-ingest.lockss.org/sourcefiles/elsevierdtd5-released/
      hidden[proxy] = DIRECT
      implicit < status ; status2 ; year ; name ; param[year] >
      
      au < readySource ; frozen ; 2014 ; Elsevier Source Content 2014 ; 2014 >
      au < readySource ; crawling ; 2015 ; Elsevier Source Content 2015 ; 2015 >
      au < readySource ; crawling ; 2016 ; Elsevier Source Content 2016 ; 2016 >
    
    }
    
  }
 
}